	"repository": {
		"type": "git",
		"url": "git@bitbucket.org:mongeralaegon/venda-digital-2.git"
	},
	"nyc": {
		"sourceMap": false,
		"instrument": false,
		"exclude": [
			"**/*.spec.js",
			"build",
			"mocha.*",
			"**/mock"
		]
	},
	"scripts": {
		"build": "webpack --config ./webpack/webpack.config.prod.js",
		"cover": "NODE_ENV=test nyc --reporter=html --reporter=text mocha --opts mocha.opts \"./app/**/*.spec.js\"",
		"clover": "NODE_ENV=test nyc --reporter=lcov > coverage/clover.lcov --reporter=text-lcov mocha --opts mocha.opts \"./app/**/*.spec.js\"",
		"dev": "babel-node server/index.js",
		"docs": "./node_modules/.bin/esdoc",
		"server": "nodemon --exec babel-node server/index.js",
		"start": "npm run server",
		"test": "NODE_ENV=test mocha --opts mocha.opts \"./app/**/*.spec.js\"",
		"test:watch": "npm run test -- --watch",
		"commit": "git-cz",
		"prestart": "npm run build",
		"postinstall": "ln -s -f ../../hooks/commit-msg .git/hooks/commit-msg && ln -s -f ../../hooks/prepare-commit-msg .git/hooks/prepare-commit-msg && rm -rf .git/hooks/commit-msg.sample && rm -rf .git/hooks/prepare-commit-msg.sample"
	},
